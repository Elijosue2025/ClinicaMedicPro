<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ClinicaMedicPro.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:vistas="clr-namespace:ClinicaMedicPro.Vistas"
    Title="ClinicaMedicPro">

    <ShellContent
        Title="Login"
        ContentTemplate="{DataTemplate vistas:LoginPage}"
        Route="LoginPage" />
    <ShellContent
    Title="Admin"
    ContentTemplate="{DataTemplate vistas:AdminPage}"
    Route="AdminPage" />
    <ShellContent
        Route="MedicoPage"
        ContentTemplate="{DataTemplate vistas:MedicoPage}" />

    <ShellContent
        Route="PacientePage"
        ContentTemplate="{DataTemplate vistas:PacientePage}" />

</Shell>

